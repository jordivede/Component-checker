{
  "name": "Component-check",
  "version": "1.0.0",
  "description": "Your Figma Plugin",
  "main": "code.js",
  "scripts": {
    "lint": "eslint --ext .js --ignore-pattern node_modules .",
    "lint:fix": "eslint --ext .js --ignore-pattern node_modules --fix ."
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@figma/eslint-plugin-figma-plugins": "*",
    "eslint": "^8.54.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@figma/figma-plugins/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "root": true,
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  }
}